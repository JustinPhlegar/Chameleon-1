<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Core-Regression" parallel="methods" thread-count="10">
	<listeners>
		<listener class-name="com.orasi.utils.debugging.Screenshot" />
	</listeners>
	<parameter name="environment" value="staging" />
	<parameter name="runLocation" value="local" />
	<parameter name="logLevel" value="TRACE" />
	<parameter name="reportToMustard" value="True" />
 
	<test name="Textbox" parallel="methods" thread-count="20">
		<classes>
			<class name="com.orasi.Sandbox" />
		</classes>
	</test>
</suite> <!-- Suite -->
